<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
	<Appeenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d - %msg%n"/>
		</Console>
	</Appeenders>
	<Loggers>
		<Root level="DEBUG">
			<AppenderRef ref="Console"/>
		</Root>
	</Loggers>
</Configuration>